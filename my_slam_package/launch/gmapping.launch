<launch>
  <!-- Launch your robot's sensor drivers (e.g., laser and odometry) -->
  <node name="scan" pkg="your_robot_driver_package" type="your_robot_driver_node" output="screen">
    <!-- Add parameters or remappings here -->
  </node>

  <!-- Launch the GMapping node -->
  <node name="gmapping" pkg="slam_gmapping" type="gmapping_node" output="screen">
    <!-- Add parameters for GMapping here -->
    <param name="map_update_interval" value="5.0"/>
    <param name="maxUrange" value="12.0"/>
    <param name="maxRange" value="16.0"/>
    <!-- Configure other parameters as needed -->
  </node>
</launch>

